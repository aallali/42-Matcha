                      <script>
                    
                 $.post('/editprofile/getInfo', function(row){
                   var data = row[0];
                   if(data !== 'Access Blocked!')
                   {
                     $(".nlikes").html(row[0].nlikes)
                  $('.fullName').html(row[0].fname + " " + row[0].lname)
                  $('#username').val(row[0].username);
                  $('#firstname').val(row[0].fname);
                  $('#lastname').val(row[0].lname);
                  $('#email').val(row[0].email);
                  $('#my-info').html(row[0].bio);
                  $('#city').val(row[0].city);
                  $('#country').val(row[0].country);

                  $('#age option[value="'+ row[0].age +'"]').attr('selected','selected');
                    if(row[0].gender === 'male')
                    $('input:radio[name=gender]')[0].checked = true;
                    else
                    $('input:radio[name=gender]')[1].checked = true;

                    if(row[0].sexpref == 0)
                    $('input:radio[name=sexPref]')[2].checked = true;
                    else if(row[0].sexpref == 1)
                    $('input:radio[name=sexPref]')[1].checked = true;
                    else
                    $('input:radio[name=sexPref]')[0].checked = true;

                   }
                  
 $('#saveAbout').on('click', function(e){
  $('#errorAbout').html('Saving Your Changes, Please Wait ... ');

  e.preventDefault();
  $('#saveAbout').prop('disabled', true);
   var data = [];
   data.push($('#username').val(),
    $('#firstname').val(),
      $('#lastname').val(),
        $('#email').val(),
         $('#age').find(":selected").val(),
         $("input[name='gender']:checked").val(),
         $("input[name='sexPref']:checked").val(),
         $('#city').val(),
         $('#country').val(),
         $('#my-info').val()
         )
   $.post('/editprofile/about', {data}, function(row){
     if(row == 'KO')
     location.reload();
    $('#saveAbout').prop('disabled', false);

    $('#errorAbout').html(row);
   })
 })
                 })
                      </script>