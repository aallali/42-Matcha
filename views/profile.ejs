  <body>
    <!-- Header
    ================================================= -->
    <%- include("header.ejs") %>
    <!--Header End-->

    <div class="container">

      <!-- Timeline
      ================================================= -->
      <div class="timeline">
        <div class="timeline-cover" style="background: url(/uploads/<%= data.pdp %> ) no-repeat;">

          <!--Timeline Menu for Large Screens-->
          <div class="timeline-nav-bar hidden-sm hidden-xs">
            <div class="row">
              <div class="col-md-3" id="divcol3f">
                <div id="leftPanel">
                  <div class="profile-info">
                    <input type="text" id="idrate" value="<%= data.id %>" hidden>
                    <input type="text" id="nstars" value="<%= data.nstars %>" hidden>

                    <img src="/uploads/<%= data.pdp %> " alt="" class="img-responsive profile-photo" />
                    <h3><%= data.fname %>  <%= data.lname %></h3>
                    <p class="text-muted">New User</p>
                    <%- data.online %>
                    <% if(data.me == 0) { %>
                    <%- include("partial/rateme.ejs", {test: 'testt'}) %>
                    <% } %>

                  
                      <div class="well">
      
                        <h4 style="color:black;font-size:25px;font-weight: 30px;"> My Fame Rate:</h4>
                        <h4 style="color:black;font-size:25px;font-weight: 30px;"><%= data.fame %> /5</h4>
                        in avrage
                   
                   
                      </div>
                  </div>
                </div>

              </div>
              <div class="col-md-9">
                <ul class="list-inline profile-menu">
          
                  <li><a></a></li>
             
                </ul>
                <ul class="follow-me list-inline">
                  <li style="font-size:20px"><b id="nlikes"><%= data.likes %> </b>
                    <% if(data.me == 1) { %>
                      likes for your profile 
                      <% } else { %>
                        Likes this profile
                        <% } %>
                    </li>
           
                  <% if(data.me == 0) { %>
                    <% if(data.liked == 0) {%>
                  <li><button class="btn-primary" style="border: solid 1px white;" id="like" onclick="Profile_Like(<%= data.id %>)">Like</button></li>
                  <li><button class="btn-primary" style="background-color: rgb(221, 4, 4);border: solid 1px white;"  id="dislike" onclick="Profile_DisLike(<%= data.id %>)" hidden>Dislike</button></li>
                  <% } else{ %>
                    <li><button class="btn-primary" style="border: solid 1px white;" id="like" onclick="Profile_Like(<%= data.id %>)" hidden>Like</button></li>
                    <li><button class="btn-primary" style="background-color: rgb(221, 4, 4);border: solid 1px white;"  id="dislike" onclick="Profile_DisLike(<%= data.id %>)">Dislike</button></li>
                    <% } %>
                    <li><button class="btn-primary" style="background-color: rgb(112, 107, 107);border: solid 1px white;" onclick="Profile_Report(<%= data.id %>)" >Report</button></li>

                  <li><button class="btn-primary" style="background-color: black;border: solid 1px white;" onclick="Profile_Block(<%= data.id %>)" >Block</button></li>

                    <% } %>
                
      

                </ul>
              </div>
            </div>
          </div><!--Timeline Menu for Large Screens End-->

          <!--Timeline Menu for Small Screens-->
          <div class="navbar-mobile hidden-lg hidden-md">
            <div class="profile-info">
              <img src="/uploads/<%= data.pdp %> " alt="" class="img-responsive profile-photo" />
              <h4><%= data.fname %>  <%= data.lname %></h4>
              <p class="text-muted">New User</p>
              <%- data.online %>
            </div>
            
            <div class="mobile-menu">
            
              <% if(data.me == 0) { %>
                <% if(data.liked == 0) {%>
              <li><button class="btn-primary" style="border: solid 1px white;" id="like2" onclick="Profile_Like(<%= data.id %>)">Like</button></li>
              <li><button class="btn-primary" style="background-color: rgb(221, 4, 4);border: solid 1px white;"  id="dislike2" onclick="Profile_DisLike(<%= data.id %>)" hidden>Dislike</button></li>

              <% } else{ %>
                <li><button class="btn-primary" style="border: solid 1px white;" id="like2" onclick="Profile_Like(<%= data.id %>)" hidden>Like</button></li>
                <li><button class="btn-primary" style="background-color: rgb(221, 4, 4);border: solid 1px white;"  id="dislike2" onclick="Profile_DisLike(<%= data.id %>)">Dislike</button></li>
