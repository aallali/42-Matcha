                <h1>sadasf</h1>
                <li><button class="btn-primary" style="background-color: rgb(76, 71, 71);border: solid 1px white;" onclick="Profile_Report(<%= data.id %>)" >Report</button></li>

              <li><button class="btn-primary" style="background-color: black;border: solid 1px white;" onclick="Profile_Block(<%= data.id %>)" >Block</button></li>


                <% } %>
                
            </div>
          </div><!--Timeline Menu for Small Screens End-->

        </div>
        <div id="page-contents">
          <div class="row">
            <div class="col-md-3" id="divcol3">
         
            </div>
            <div class="col-md-7">

              <!-- About
              ================================================= -->
              <div class="about-profile">
                <div class="about-content-block">
                
                  <h4 class="grey"><i class="ion-ios-information-outline icon-in-title"></i>Personal Information</h4>
                  <div class="row">
                      <div class="col-md-3">
                        <h4><b style="color:black"> First Name :</b><br></h4>
                      </div>
                      <div class="col-md-6">
                        <input type="text" class="form-control" value="<%= data.fname %> " style="color:black;font-size: 20px;" disabled>
                  
                    </div>                      
                  </div>

                  <div class="row">
                    <div class="col-md-3">
                      <h4><b style="color:black"> Last Name :</b><br></h4>
                    </div>
                    <div class="col-md-6">
                      <input type="text" class="form-control" value="<%= data.lname %> " style="color:black;font-size: 20px;" disabled>
                
                  </div>                      
                </div>
                  <div class="row">
                    <div class="col-md-3">
                      <h4><b style="color:black"> Username :</b><br></h4>
                    </div>
                    <div class="col-md-6">
                      <input type="text" class="form-control" value="<%= data.username %> " style="color:black;font-size: 20px;" disabled>
                  </div>                      
                </div>
                <div class="row">
                  <div class="col-md-3">
                    <h4><b style="color:black"> Age :</b><br></h4>
                  </div>
                  <div class="col-md-6">
                    <input type="text" class="form-control" value="<%= data.age %> y.o" style="color:black;font-size: 20px;" disabled>
                </div>                      
              </div>   
              <div class="row">
                <div class="col-md-3">
                  <h4><b style="color:black"> Gender :</b><br></h4>
                </div>
                <div class="col-md-6">
                  <input type="text" class="form-control" value="<%= data.gender %> " style="color:black;font-size: 20px;" disabled>
              </div>                      
            </div>                  
                </div>
                </div>
                <div class="about-content-block">
                    <h4 class="grey"><i class="ion-ios-information-outline icon-in-title"></i>Biography</h4>
                    <textarea type="text" class="form-control" style="height:200px;color:black;font-size: 20px;" disabled><%= data.bio %> </textarea> 
                  </div>
                  <hr>
                <div class="about-content-block">
                  <h4 class="grey"><i class="ion-ios-location-outline icon-in-title"></i>Location</h4>
                  <p><%= data.city %> <%= data.country %> </p>
                    <div id="map" style="height: 400px;"></div>
                    <input id="lat" value="<%= data.lat %> " hidden>
                    <input id="lon" value="<%= data.lon %> " hidden>
                </div>
                <hr>
                <div class="row">
                <div class="about-content-block">
                  <h4 class="grey"><i class="ion-ios-heart-outline icon-in-title"></i>Interests</h4>
                  <% data.tags.forEach(el=> { %>
                    <div class="tagLabel"><p>  <%=  el.tag %></p></div>
                <% }) %>
                  <ul class="interests list-inline">
                    <li ></li>
                  </ul>
                </div>
              </div>
                <hr> 
                <div class="about-content-block">
                    <h4 class="grey"><i class="icon ion-images icon-in-title"></i>Album</h4>
                    <ul class="album-photos">
                      <% var i = 0; data.album.forEach(el=> { %>
                        <li>
                          <div class="img-wrapper" data-toggle="modal" data-target=".photo-<%=i%>">
                            <img class="toresize" src="/uploads/<%=  el.name %>" alt="photo"/>
                          </div>
                          <div class="modal fade photo-<%=i%>" tabindex="-1" role="dialog" aria-hidden="true">
                            <div class="modal-dialog modal-lg">
                              <div class="modal-content">
                                <img src="/uploads/<%=  el.name %>" alt="photo" />
                              </div>
                            </div>
                          </div>
                        </li>
                    <% i++;}) %>
                      </ul>
                  </div>
              </div>
            
            <div class="col-md-2 static">
              <div id="sticky-sidebar" class="historybar">
                <h4 class="grey"><%= data.fname %>  <%= data.lname %>'s activity</h4>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>


    <!-- Footer
    ================================================= -->
    <%- include("partial/footer.ejs") %>
    
    <!--preloader-->
    <div id="spinner-wrapper">
      <div class="spinner"></div>
    </div>

